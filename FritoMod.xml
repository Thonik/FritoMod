<Ui xmlns="http://www.blizzard.com/wow/ui/">
	<!-- A tooltip used in FritoMod. This tooltip is not threadsafe, so its
		 contents may be changed between calls. -->
	<GameTooltip name="FritoModTooltip" inherits="GameTooltipTemplate">
		<Scripts>
			<Onload>
				self:SetOwner(WorldFrame, "ANCHOR_NONE");
			</Onload>
		</Scripts>
	</GameTooltip>
</Ui>
